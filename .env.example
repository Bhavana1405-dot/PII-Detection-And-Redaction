# Environment variables for PII Redaction System

# Paths
REDACTION_INPUT_DIR=./inputs
REDACTION_OUTPUT_DIR=./outputs
OCTOPII_PATH=./Octopii
# Configuration
REDACTION_CONFIG=./Redactopii/configs/default_config.json
LOG_LEVEL=INFO

# Security
ENABLE_ENCRYPTION=false
KEY_STORAGE_PATH=./keys

# Performance
MAX_FILE_SIZE_MB=100
BATCH_SIZE=50